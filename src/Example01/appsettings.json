{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Warning"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "HH:mm:ss.ffff "
      }
    }
  },
  "Settings": {
    "RetryCount": 3,
    "RetryDelayInSeconds": 1,
    "ConsumerDelayInSeconds": 1,
    "ProducerDelayInSeconds": 5,
    "BrokerType": "InMemory",
    "RabbitMq":{
      "Url": "localhost",
      "UserName": "guest",
      "UserPass": "guest"
    },
    "ServiceBus": {
      "ConnectionString": "Endpoint=sb://namespace.service-bus.windows.net/"
    }
  }
}
